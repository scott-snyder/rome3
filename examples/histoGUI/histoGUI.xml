<ROMEFrameworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="c:/rome/rome.xsd">
	<Experiment>
		<ExperimentShortCut>HGUI</ExperimentShortCut>
		<ProgramName>Example</ProgramName>
	</Experiment>
	<Tasks>
		<Task>
			<TaskName>FillHisto</TaskName>
			<Histogram>
				<HistName>MyHisto</HistName>
				<HistFolderName>myhistos</HistFolderName>
				<HistType>TH1F</HistType>
				<HistArraySize>4</HistArraySize>
				<HistXLabel>X</HistXLabel>
				<HistYLabel>Entries</HistYLabel>
				<HistXNbins>100</HistXNbins>
				<HistXmin>-100</HistXmin>
				<HistXmax>100</HistXmax>
				<Argus>
					<Tab>
						<TabName>GeneratedHisto</TabName>
						<ObjectArrayIndex>0-1</ObjectArrayIndex>
						<Index>0</Index>
					</Tab>
					<Tab>
						<TabName>GeneratedHisto</TabName>
						<ObjectArrayIndex>2-3</ObjectArrayIndex>
						<Index>2</Index>
						<DrawSamePad>true</DrawSamePad>
					</Tab>
				</Argus>
			</Histogram>
			<Graph>
				<GraphName>MyGraph</GraphName>
				<GraphFolderName>mygraphs</GraphFolderName>
				<GraphType>TGraph</GraphType>
				<GraphXLabel>X</GraphXLabel>
				<GraphYLabel>Y</GraphYLabel>
				<GraphXmin>0</GraphXmin>
				<GraphXmax>4</GraphXmax>
				<GraphYmin>-2</GraphYmin>
				<GraphYmax>2</GraphYmax>
				<Argus>
					<Tab>
						<TabName>GeneratedHisto</TabName>
						<Index>4</Index>
					</Tab>
				</Argus>
			</Graph>
			<Graph>
				<GraphName>MyGraphError</GraphName>
				<GraphFolderName>mygraphs</GraphFolderName>
				<GraphType>TGraphErrors</GraphType>
				<GraphXLabel>X</GraphXLabel>
				<GraphYLabel>Y</GraphYLabel>
				<GraphXmin>0</GraphXmin>
				<GraphXmax>4</GraphXmax>
				<GraphYmin>-2</GraphYmin>
				<GraphYmax>2</GraphYmax>
				<Argus>
					<Tab>
						<TabName>GeneratedHisto</TabName>
						<Index>5</Index>
					</Tab>
				</Argus>
			</Graph>
			<Histogram>
				<HistName>MyOtherHisto</HistName>
				<HistFolderName>myotherhistos</HistFolderName>
				<HistType>TH1F</HistType>
				<HistXLabel>X</HistXLabel>
				<HistYLabel>Entries</HistYLabel>
				<HistXNbins>100</HistXNbins>
				<HistXmin>-100</HistXmin>
				<HistXmax>100</HistXmax>
				<Argus>
					<Tab>
						<TabName>GeneratedHisto</TabName>
						<Index>3</Index>
					</Tab>
				</Argus>
			</Histogram>
			<Histogram>
				<HistName>My2DHisto</HistName>
				<HistFolderName>myhistos</HistFolderName>
				<HistType>TH2F</HistType>
				<HistArraySize>2</HistArraySize>
				<HistXLabel>X</HistXLabel>
				<HistYLabel>Y</HistYLabel>
				<HistXNbins>10</HistXNbins>
				<HistXmin>-100</HistXmin>
				<HistXmax>100</HistXmax>
				<HistYNbins>10</HistYNbins>
				<HistYmin>-100</HistYmin>
				<HistYmax>100</HistYmax>
				<Argus>
					<Tab>
						<TabName>GeneratedHisto</TabName>
						<ObjectArrayIndex>0-1</ObjectArrayIndex>
						<Index>6</Index>
					</Tab>
				</Argus>
			</Histogram>
		</Task>
	</Tasks>
</ROMEFrameworkDefinition>
